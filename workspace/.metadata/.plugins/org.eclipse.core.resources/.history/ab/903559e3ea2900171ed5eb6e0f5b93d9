package travelingSalesman;

import java.util.ArrayList;
import java.util.Scanner;

import autoSink.AutoSink.Vertex;

public class TravelingSalesman {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		String input = scanner.nextLine();
		
		int numVertecies = Integer.parseInt(input);
		ArrayList<ArrayList<Integer>> edges = new ArrayList<ArrayList<Integer>>();
		for(int i = 0; i < numVertecies; i++){
			String newCityAToll = scanner.nextLine();
			String[] indivInfo = newCityAToll.split(" ");
			edges.add(new Vertex(indivInfo[0], Integer.parseInt(indivInfo[1])));
		}
	}

}
